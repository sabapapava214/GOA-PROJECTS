name = "saba"
#name არის ცვლადი
#name = არის ცვლადისთვის მნიშვნელოვნების მინიჭებული სიმბოლო
#"nika" არის ცვლადისტვის მნიშვნელობა

surname = "papava"

#print "name"
#პრინტ ფუნქციას გადაეცემა ეკრანზე გამოსატანი ობიექტი

print (name) #ეს არის str (string) ტიპის ცვლადი
age = 12 #ეს არის int (integer) მთელი რიცხვი
height = 148.5 #ეს არის float ტიპის ცვლადი (ათწილადი)
#boolean (bool) ტიპის ცვლადი

knows_programing = True #true ან false
isugly = False #ჯავასკრიპტული camelcase

print(name + surname)

print(+ " " + surname)
 #სტრინგი არის ბრჭყალებში მოქცეულისიმბოლო
print(name + age) 

print(type(age))
print(type(name))
print(type(surname))
print(type(height))
print(type(knows_programing))

print(name + " " + str(age)) 